{
  "name": "release-quest",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Multiplayer bug-catching arcade game",
  "scripts": {
    "start": "node server.ts",
    "build": "node build.mjs && vite build --config frontend/vite.config.ts",
    "build:legacy": "node build.mjs",
    "build:client": "vite build --config frontend/vite.config.ts",
    "dev": "node build.mjs --watch",
    "dev:client": "vite --config frontend/vite.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=24"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "pg": "^8.13.0",
    "vue": "^3.5.28",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.0",
    "@types/ws": "^8.5.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "esbuild": "^0.27.3",
    "typescript": "^5.8.0",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.4"
  }
}
