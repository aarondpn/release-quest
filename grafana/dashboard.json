{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{instance=~\"$instance\"}[5m])) by (method)",
          "legendFormat": "{{method}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      }
    },
    {
      "title": "Request Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{instance=~\"$instance\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{instance=~\"$instance\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{instance=~\"$instance\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    },
    {
      "title": "Active WebSocket Connections",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "ws_connections_active{instance=~\"$instance\"}",
          "legendFormat": "Active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "title": "WS Messages/sec",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "sum(rate(ws_messages_received_total{instance=~\"$instance\"}[5m]))",
          "legendFormat": "Received",
          "refId": "A"
        },
        {
          "expr": "rate(ws_messages_sent_total{instance=~\"$instance\"}[5m])",
          "legendFormat": "Sent",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "msg/s"
        },
        "overrides": []
      }
    },
    {
      "title": "Active Lobbies",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 8 },
      "targets": [
        {
          "expr": "game_lobbies_active{instance=~\"$instance\"}",
          "legendFormat": "Lobbies",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "title": "Players Online",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "game_players_online{instance=~\"$instance\"}",
          "legendFormat": "Players",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "title": "Games Started / Completed",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "sum(rate(game_games_started_total{instance=~\"$instance\"}[5m]))",
          "legendFormat": "Started",
          "refId": "A"
        },
        {
          "expr": "sum(rate(game_games_completed_total{instance=~\"$instance\"}[5m]))",
          "legendFormat": "Completed",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "games/s"
        },
        "overrides": []
      }
    },
    {
      "title": "Win Rate",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 12 },
      "targets": [
        {
          "expr": "sum(rate(game_games_completed_total{outcome=\"win\", instance=~\"$instance\"}[5m])) / sum(rate(game_games_completed_total{instance=~\"$instance\"}[5m]))",
          "legendFormat": "Win Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.3 },
              { "color": "green", "value": 0.6 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Bugs Squashed/sec",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "rate(game_bugs_squashed_total{instance=~\"$instance\"}[5m])",
          "legendFormat": "Bugs Squashed",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bugs/s"
        },
        "overrides": []
      }
    },
    {
      "title": "Process Memory",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes{instance=~\"$instance\"}",
          "legendFormat": "RSS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      }
    },
    {
      "title": "CPU Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{instance=~\"$instance\"}[5m])",
          "legendFormat": "CPU",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      }
    },
    {
      "title": "Event Loop Lag",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "targets": [
        {
          "expr": "nodejs_eventloop_lag_seconds{instance=~\"$instance\"}",
          "legendFormat": "Lag",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["release-quest"],
  "templating": {
    "list": [
      {
        "current": {},
        "definition": "label_values(up, instance)",
        "includeAll": true,
        "multi": true,
        "name": "instance",
        "query": "label_values(up, instance)",
        "refresh": 2,
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Release Quest",
  "uid": "release-quest",
  "version": 1
}
